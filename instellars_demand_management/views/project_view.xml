<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_project_form_inherit_demands" model="ir.ui.view">
        <field name="name">project.project.form.inherit.demands</field>
        <field name="model">project.project</field>
        <field name="inherit_id" ref="project.edit_project"/>
        <field name="arch" type="xml">
            <xpath expr="//form/sheet/div[@name='button_box']" position="inside">
                    <button class="oe_stat_button" type="object" name="get_demand_list" icon="fa-users">
                        <field string="Demands" name="demands_count" widget="statinfo" />
                       
                    </button>
                </xpath>
        </field>
    </record>
    
</odoo>